{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'User',\n  computed: {\n    search() {\n      return search;\n    }\n  },\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      PageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          PageNum: this.currentPage,\n          PageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: \"更新成功\"\n            });\n            sessionStorage.setItem(\"user\", JSON.stringify(this.form));\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false;\n        });\n      } else {\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false;\n        });\n      }\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleTodo(row) {\n      this.$router.push({\n        path: '/todo',\n        query: {\n          userId: row.id\n        }\n      });\n    },\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          });\n        }\n        this.load();\n      });\n    },\n    handleSizeChange(PageSize) {\n      //改变当前每页个数触发\n      this.pageSize = PageSize;\n      this.load();\n    },\n    handleCurrentChange(PageNum) {\n      //改变当前页码触发\n      this.currentPage = PageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","name","computed","search","components","data","form","dialogVisible","currentPage","PageSize","total","tableData","created","load","methods","get","params","PageNum","pageSize","then","res","console","log","records","add","save","id","put","code","$message","type","message","sessionStorage","setItem","JSON","stringify","msg","post","handleEdit","row","parse","handleTodo","$router","push","path","query","userId","handleDelete","delete","handleSizeChange","handleCurrentChange"],"sources":["C:\\Users\\哈振宇\\Desktop\\软件工程\\springboot+vue框架\\demo\\vue\\src\\views\\UserView.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n    <!--    功能区-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n    <!--    搜索区-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" style=\"width: 20%\" placeholder=\"请输入关键字\" clearable/>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"ID\" width=\"100\"/>\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"150\"/>\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"150\"/>\n      <el-table-column prop=\"age\" label=\"年龄\" width=\"150\"/>\n      <el-table-column prop=\"sex\" label=\"性别\" width=\"100\"/>\n      <el-table-column prop=\"address\" label=\"地址\" width=\"150\"/>\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"250\">\n        <template #default=\"scope\">\n          <el-button size=\"small\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-button size=\"small\" style=\"width: 100px\" type=\"primary\" @click=\"handleTodo(scope.row)\">待办事件</el-button>\n          <el-popconfirm title=\"确定删除这一段内容吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"small\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          v-model:current-page=\"currentPage\"\n          v-model:page-size=\"pageSize\"\n          :page-sizes=\"[5, 10, 20]\"\n          :size=\"PageSize\"\n          :disabled=\"disabled\"\n          :background=\"background\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n      />\n      <el-dialog v-model=\"dialogVisible\" title=\"提示\" width=\"30%\">\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickname\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model.number=\"form.age\" style=\"width: 80%\"/>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio v-model=\"form.sex\" value=\"男\">男</el-radio>\n            <el-radio v-model=\"form.sex\" value=\"女\">女</el-radio>\n            <el-radio v-model=\"form.sex\" value=\"未知\">未知</el-radio>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width: 80%\"/>\n          </el-form-item>\n\n        </el-form>\n        <template #footer>\n          <div class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">\n              确 定\n            </el-button>\n          </div>\n        </template>\n      </el-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'User',\n  computed: {\n    search() {\n      return search\n    }\n  },\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      currentPage: 1,\n      PageSize: 10,\n      total: 0,\n      tableData: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          PageNum: this.currentPage,\n          PageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res)\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: \"更新成功\"\n            })\n            sessionStorage.setItem(\"user\", JSON.stringify(this.form))\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            })\n          }\n          this.load()  //刷新表格数据\n          this.dialogVisible = false\n        })\n      } else {\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: 'success',\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: 'error',\n              message: res.msg\n            })\n          }\n          this.load()  //刷新表格数据\n          this.dialogVisible = false\n        })\n      }\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleTodo(row) {\n      this.$router.push({ path: '/todo', query: { userId: row.id } });\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: 'success',\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: 'error',\n            message: res.msg\n          })\n        }\n        this.load()\n      })\n    },\n    handleSizeChange(PageSize) {  //改变当前每页个数触发\n      this.pageSize = PageSize\n      this.load()\n    },\n    handleCurrentChange(PageNum) { //改变当前页码触发\n      this.currentPage = PageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":";AAgFA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,QAAQ,EAAE;IACRC,MAAMA,CAAA,EAAG;MACP,OAAOA,MAAK;IACd;EACF,CAAC;EACDC,UAAU,EAAE,CAAC,CAAC;EAEdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,aAAa,EAAE,KAAK;MACpBJ,MAAM,EAAE,EAAE;MACVK,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,